{
    "name": "craftcms/craft6-prepper",
    "description": "Craft 6 project prepper",
    "keywords": [
        "craftcms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Brandon Kelly",
            "email": "brandon@pixelandtonic.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "composer/semver": "^3.4",
        "illuminate/filesystem": "^10.20|^11.0|^12.0",
        "illuminate/support": "^10.20|^11.0|^12.0",
        "laravel/prompts": "^0.1.18|^0.2.0|^0.3.0",
        "symfony/console": "^6.2|^7.0",
        "vlucas/phpdotenv": "^4.0|^5.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^2.1",
        "phpunit/phpunit": "^10.4"
    },
    "bin": [
        "bin/craft6-prep"
    ],
    "autoload": {
        "psr-4": {
            "CraftCms\\Prepper\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CraftCms\\Prepper\\Console\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
